{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryforovi"
		},
		"AzureSqlDatabaseLINK_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLINK'"
		},
		"AzureSynapseAnalyticsLINK_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalyticsLINK'"
		},
		"appvm_intergratedruntimeSERVICE_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvm_intergratedruntimeSERVICE'"
		},
		"datalakeLINK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeLINK'"
		},
		"linkedServiceSTAGING_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedServiceSTAGING'"
		},
		"appvm_intergratedruntimeSERVICE_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.24.0\\logs"
		},
		"appvm_intergratedruntimeSERVICE_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "appusr"
		},
		"datalakeLINK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeovi.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryforovi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLINK_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLINK_connectionString')]"
					},
					"AzureSynapseAnalyticsLINK_connectionString": {
						"value": "[parameters('AzureSynapseAnalyticsLINK_connectionString')]"
					},
					"datalakeLINK_accountKey": {
						"value": "[parameters('datalakeLINK_accountKey')]"
					},
					"linkedServiceSTAGING_connectionString": {
						"value": "[parameters('linkedServiceSTAGING_connectionString')]"
					},
					"appvm_intergratedruntimeSERVICE_password": {
						"value": "[parameters('appvm_intergratedruntimeSERVICE_password')]"
					},
					"datalakeLINK_properties_typeProperties_url": {
						"value": "[parameters('datalakeLINK_properties_typeProperties_url')]"
					},
					"appvm_intergratedruntimeSERVICE_properties_typeProperties_host": {
						"value": "[parameters('appvm_intergratedruntimeSERVICE_properties_typeProperties_host')]"
					},
					"appvm_intergratedruntimeSERVICE_properties_typeProperties_userId": {
						"value": "[parameters('appvm_intergratedruntimeSERVICE_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactoryforovi_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryforovi_ArmTemplate_0')]"
			]
		}
	]
}